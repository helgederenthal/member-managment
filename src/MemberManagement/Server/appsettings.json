{
  "ConnectionStrings": {
    "DatabaseConnection": "Data Source=MemberManagementDatabase.db;"
  },
  "DemoData": {
    "ClearDatabase": "true",
    "AddDemoData": "true"
  },
  "Serilog": {
    "MinimulLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
